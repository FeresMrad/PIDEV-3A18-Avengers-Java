<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ChatController">


<VBox fx:id="chatBox" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="361.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
            <VBox fx:id="messageBox" prefHeight="0.0" prefWidth="580.0" />
        </ScrollPane>
        <HBox prefHeight="39.0" prefWidth="600.0" spacing="5.0">
            <TextField fx:id="messageInput" prefWidth="500.0" />
            <Button fx:id="sendButton" mnemonicParsing="false" text="Send" />
        </HBox>
    </children>
</VBox>
</AnchorPane>
